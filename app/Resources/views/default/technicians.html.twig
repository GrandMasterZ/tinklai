{% extends 'base.html.twig' %}

{% block body %}

    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>Vardas</th>
            <th>Pavardė</th>
            <th>Vartotojo vardas</th>
            <th>Elektroninis paštas</th>
            <th> Užpildytos ataskaitos</th>
        </tr>
        </thead>
        <tbody>
        {% for user in technicians %}
            <tr>
                <td> {{ user.technician_data.name }} </td>
                <td> {{ user.technician_data.surname }} </td>
                <td> {{ user.technician_data.username }} </td>
                <td> {{ user.technician_data.email }} </td>
                <td> {{ user.reportCount }} </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}